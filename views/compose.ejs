<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class=form-group>
            <label for="title">Title</label>
            <input class="form-control" type="text" name="postTitle" autocomplete="off">
            <label for="text">Post</label>
            <textarea class="form-control" name="postBody" id="" cols="30" rows="5"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button" value="">Publish</button>
        
</form> 

